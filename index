            <script>
                function reload_catpcha() {
                    document.getElementById('captcha_image').src = 'https://camisetasargentinas.com/security/securimage_show.php?namespace=editor_camisetasargentinascom&' + Math.random();
                    captcha_image_audioObj.refresh();
                    this.blur();
                    $("#captcha_code").val("");
                    return false
                }
                $.fn.isInViewport = function() {
                    var elementTop = $(this).offset().top;
                    var elementBottom = elementTop + $(this).outerHeight();
                    var viewportTop = $(window).scrollTop();
                    var viewportBottom = viewportTop + $(window).height();
                    return elementBottom > viewportTop && elementTop < viewportBottom;
                };

                function isInArray(value, array) {
                    return array.indexOf(value) > -1;
                }

                // $(window).on('load', function() {
                //     $('#loading').hide();
                // })
                window.page_loaded = 0;
                var drags = [];
                var interval = null;
                var item_edit = null;
                var block_edit = null;
                window.counter = [];

                $(window).on('resize scroll', function() {
                    $('.counter').each(function() {
                        var countTo = $(this).attr('count_to');
                        var id_counter = $(this).attr('id');
                        if ($(this).isInViewport()) {
                            if (!isInArray(id_counter, window.counter)) {
                                // console.log(countTo);
                                window.counter.push(id_counter);
                                if ($('#' + id_counter)) {
                                    const countUp = new CountUp(id_counter, countTo);
                                    if (!countUp.error) {
                                        countUp.start();
                                    } else {
                                        console.error(countUp.error);
                                    }
                                }
                            }
                        }
                    });
                });

                $(document).ready(function() {

                    if (document.getElementById('typed')) {
                        var typed = new Typed("#typed", {
                            stringsElement: '#typed-strings',
                            typeSpeed: 70,
                            backSpeed: 50,
                            backDelay: 200,
                            startDelay: 500,
                            loop: true
                        });
                    }

                    $('.ajax_site').each(function(d, obj) {

                        var func = $(this).attr("f");

                        var site = $(this);

                        site.html($('#div_loading').html());

                        var explode = $(this).attr("attributes").split(",");

                        var parameters = {};

                        for (let i = 0; i < explode.length; i++) {
                            parameters[explode[i]] = $(this).attr(explode[i]);
                        }

                        // parameters["escaped_link"] = "<php echo $escaped_link; ?>";

                        $.ajax({
                            data: parameters,
                            type: "POST",
                            url: "https://camisetasargentinas.com/ajax.php",
                            success: function(data) {
                                site.html(data);
                                start_blocks();
                            }
                        })
                    });
                });

                $("html").on("click", ".add_product", function() {

                    drawer_left(1);
                    var qty_product = $("#qty_product").val();
                    var id_product = $(this).attr("id_product");
                    var id_var_1 = $(this).attr("id_var_1");
                    var id_var_2 = $(this).attr("id_var_2");
                    var id_var_3 = $(this).attr("id_var_3");

                    var parameters = {
                        f: "add_product",
                        id_product: id_product,
                        id_var_1: id_var_1,
                        id_var_2: id_var_2,
                        id_var_3: id_var_3,
                        qty_product: qty_product,
                    };

                    $.ajax({
                        data: parameters,
                        type: "POST",
                        url: "https://camisetasargentinas.com/ajax.php",
                        success: function(data) {
                            // alert(data);
                            get_cart();
                            reload_cart();
                        }
                    })

                    return false;
                });

                $("html").on("click", ".link_variant", function() {
                    $('#loading-page').html('<div class="progress-css"></div>');
                    var href = $(this).attr("href");
                    window.history.pushState("", "", href);
                    start_blocks();
                    return false;
                });

                function start_block(div) {

                    // div.html($('#div_loading').html());

                    var store = div.attr("store");
                    var header_image = div.attr("header_image");

                    var explode = div.attr("attributes").split(",");

                    var parameters = {};

                    for (let i = 0; i < explode.length; i++) {
                        parameters[explode[i]] = div.attr(explode[i]);
                    }

                    parameters["escaped_link"] = "https://camisetasargentinas.com/";

                    $.ajax({
                        data: parameters,
                        type: "POST",
                        url: "https://camisetasargentinas.com/ajax.php",
                        success: function(data) {
                            if (store == 1) {
                                if (window.page_loaded == 0) {
                                    window.page_loaded = 1;
                                    $('.main_page').show();
                                    $('.loading_page').fadeOut(null);
                                    setTimeout(function() {
                                        $('body').css("overflow-y", "auto");
                                    }, 500);
                                }
                            } else if (header_image == 1) {
                                if (window.page_loaded == 0) {
                                    setTimeout(function() {
                                        window.page_loaded = 1;
                                        $('.main_page').show();
                                        $('.loading_page').fadeOut(null);
                                        setTimeout(function() {
                                            $('body').css("overflow-y", "auto");
                                        }, 500);
                                    }, 800);
                                }
                            }
                            div.html(data);
                            each_items(parameters["html_block"]);
                            init_material();
                            each_iframe();
                            // if ($('.js-slick-carousel').length) {
                            //     // $('.js-slick-carousel').slick({});
                            //     $.HSSlickCarousel.init('.js-slick-carousel');
                            // }
                        }
                    })
                }

                function decode_iframes() {
                    $('.altagama-iframe').each(function(d, obj) {
                        var src = $(this).attr('src');
                        var decode = decodeURIComponent(src);
                        if (src != decode) {
                            $(this).attr('src', decode);
                        }
                    })
                }

                function start_blocks() {

                    $('.ajax_block').each(function(d, obj) {
                        start_block($(this));
                    });
                }

                function start_item(div, html_block = null) {

                    var explode = div.attr("attributes").split(",");

                    var parameters = {};

                    for (let i = 0; i < explode.length; i++) {
                        parameters[explode[i]] = div.attr(explode[i]);
                    }

                    $.ajax({
                        data: parameters,
                        type: "POST",
                        url: "https://camisetasargentinas.com/ajax.php",
                        success: function(data) {
                            div.html(data);
                            if (html_block == 1) {
                                if (window.page_loaded == 0) {
                                    window.page_loaded = 1;
                                    $('.main_page').show();
                                    $('.loading_page').fadeOut(null);
                                    setTimeout(function() {
                                        $('body').css("overflow-y", "auto");
                                    }, 1000);
                                }
                            }
                            check_delivery_type();
                            init_material();
                            init_choices();
                            reload_counters();
                            each_iframe();
                            decode_iframes();
                            $('.popup-gallery').magnificPopup({
                                delegate: 'a',
                                type: 'image',
                                tLoading: 'Cargando imagen #%curr%...',
                                mainClass: 'mfp-img-mobile',
                                gallery: {
                                    enabled: true,
                                    navigateByImgClick: true,
                                    preload: [0, 1] // Will preload 0 - before current, and 1 after the current image
                                },
                                image: {
                                    tError: '<a href="%url%">La imagen #%curr%</a> no se pudo cargar.',
                                }
                            });
                        }
                    })
                }

                function each_items(html_block) {

                    $('.ajax_item[html_block="' + html_block + '"]').each(function(d, obj) {
                        start_item($(this), html_block)
                    });
                }

                function init_choices() {

                    // const element = document.querySelector('.choice');

                    $('.choice').each(function(d, obj) {

                        // console.log($(this)[0].name);

                        // const element = document.querySelector('.choice');

                        $(this).toggleClass('choice choice_ok');

                        const element = $(this)[0];

                        const choices = new Choices(element, {
                            position: 'auto',
                            resetScrollPosition: true,
                            placeholderValue: 'Seleccionar...',
                            searchPlaceholderValue: "Buscar...",
                            loadingText: 'Cargando...',
                            noResultsText: 'Sin resultados',
                            noChoicesText: 'No hay opciones para elegir',
                            itemSelectText: 'Presione para seleccionar',
                        });
                    });
                }

                $("html").on("click", ".link_checkout", function() {
                    var href = $(this).attr("href");
                    window.history.pushState("", "", href);
                    item_edit = 1;
                    reload_item();
                    return false;
                });

                $("html").on("submit", "#form_newsletter", function() {

                    var email = $("#newsletter_email").val();

                    if (!validateEmail(email)) {
                        swal("Email Invalido", "Ingresa tu email correctamente.", "error");
                        return false;
                    }

                    var data = $(this).serialize();

                    // console.log(window.error);

                    $.ajax({
                        data: data,
                        type: "POST",
                        url: "https://camisetasargentinas.com/ajax.php",
                        success: function(data) {
                            if (data == "canceled") {
                                $('#title_modal_clean').html('El newsletter está deshabilitado');
                                $('#div_modal_clean').html('<div style="text-align:center;margin-top: 10px;margin-bottom: 10px;"><div class="alert alert-danger" role="alert"><p style="font-weight: bold;font-size: 1.3rem;color: white;margin-bottom: 5px;">No puedes suscribirte.</p><p style="margin-bottom: 0;font-size: 1rem;color: white;">Lo sentimos, el newsletter se encuentra deshabilitado.</p></div></div>');
                                $('#modal_clean').modal('toggle');
                                document.getElementById("form_newsletter").reset();
                            } else {
                                swal("Gracias por suscribirte", "En breve recibiras nuestras promociones.", "success");
                                document.getElementById("form_newsletter").reset();
                            }
                            return false;
                        }
                    })

                    return false;
                })

                $("html").on("submit", "#form_contact", function() {

                    var email = $("#contact_email").val();

                    if (!validateEmail(email)) {
                        swal("Email Invalido", "Ingresa tu email correctamente.", "error");
                        return false;
                    }

                    var data = $(this).serialize();

                    // console.log(window.error);

                    $.ajax({
                        data: data,
                        type: "POST",
                        url: "https://camisetasargentinas.com/ajax.php",
                        success: function(data) {
                            if (data == "no") {
                                swal("Captcha incorrecto", "Ingrese el captcha correctamente.", "error");
                                reload_catpcha();
                            } else if (data == "canceled") {
                                $('#title_modal_clean').html('El formulario está deshabilitado');
                                $('#div_modal_clean').html('<div style="text-align:center;margin-top: 10px;margin-bottom: 10px;"><div class="alert alert-danger" role="alert"><p style="font-weight: bold;font-size: 1.3rem;color: white;margin-bottom: 5px;">No podemos recibir mensajes.</p><p style="margin-bottom: 0;font-size: 1rem;color: white;">Lo sentimos, el formulario de contacto se encuentra deshabilitado.</p></div></div>');
                                $('#modal_clean').modal('toggle');
                                document.getElementById("form_contact").reset();
                                reload_catpcha();
                            } else {
                                swal("Mensaje enviado", "En breve te responderemos", "success");
                                document.getElementById("form_contact").reset();
                                reload_catpcha();
                            }
                            return false;
                        }
                    })

                    return false;
                })

                $("html").on("submit", "#form_checkout", function() {

                    var data = $(this).serialize();

                    window.error = 0;

                    $('.checkout_fields').each(function(d, obj) {

                        var is_email = $(this).attr("is_email");
                        var is_required = $(this).attr("is_required");
                        var field = $(this).attr("field");
                        var val = $(this).val();

                        if (is_required == 1 && val.length == 0) {
                            swal(
                                field + " vacio",
                                "Es un campo requerido.",
                                "error"
                            );
                            window.error = 1;
                            return false;
                        }
                        if (is_email == 1) {
                            if (!validateEmail(val)) {
                                swal("Email Invalido", "Ingresa tu email correctamente.", "error");
                                window.error = 1;
                                return false;
                            }
                        }
                    });

                    // console.log(window.error);

                    if (window.error == 0) {
                        $.ajax({
                            data: data,
                            type: "POST",
                            url: "https://camisetasargentinas.com/ajax.php",
                            success: function(data) {
                                // alert(data);
                                window.history.pushState("", "", "/finalizar-compra/pagar/");
                                item_edit = 1;
                                reload_item();
                                reload_resume();
                                $("html,body").animate({
                                        scrollTop: $($(".card-body")).offset().top,
                                    },
                                    0
                                );
                                return false;
                            }
                        })
                    }
                    return false;
                })

                $("html").on("submit", "#form_delivery", function() {

                    var data = $(this).serialize();
                    window.error = 0;

                    $('.delivery_fields').each(function(d, obj) {

                        var is_required = $(this).attr("is_required");
                        var field = $(this).attr("field");
                        var val = $(this).val();

                        if (is_required == 1 && val.length == 0) {
                            swal(
                                field + " vacio",
                                "Es un campo requerido.",
                                "error"
                            );
                            window.error = 1;
                            return false;
                        }
                    });

                    // console.log(window.error);

                    if (window.error == 0) {
                        $.ajax({
                            data: data,
                            type: "POST",
                            url: "https://camisetasargentinas.com/ajax.php",
                            success: function(data) {
                                window.history.pushState("", "", "/finalizar-compra/datos/");
                                item_edit = 1;
                                reload_item();
                                reload_resume();
                                $("html,body").animate({
                                        scrollTop: $($(".card-body")).offset().top,
                                    },
                                    0
                                );
                                return false;
                            }
                        })
                    }
                    return false;
                })


                function check_delivery_options() {


                    is_checked = $("input[name='check_takeaway_options']").is(':checked');

                    if (is_checked) {

                        var value = $("input[name='check_takeaway_options']:checked").val();

                        // console.log("check_takeaway_options");
                        // console.log("value: " + value);

                        var parameters = {
                            f: "define_delivery_options",
                            val: value,
                            name: "takeaway_options",
                        };

                        $.ajax({
                            data: parameters,
                            type: "POST",
                            url: "https://camisetasargentinas.com/ajax.php",
                            success: function(data) {
                                $('#div_fields_delivery').html(data);
                                init_material();
                                init_choices();
                            }
                        })
                    }

                    // console.log($("input[name='check_takeaway_options']:checked").val());

                }

                function select_delivery_options() {

                    if ($("#select_delivery_options").length) {

                        var value = $("#select_delivery_options").val();

                        // console.log("select_delivery_options");
                        // console.log("value: " + value);

                        var parameters = {
                            f: "define_delivery_options",
                            val: value,
                            name: "shipping_options",
                        };

                        $.ajax({
                            data: parameters,
                            type: "POST",
                            url: "https://camisetasargentinas.com/ajax.php",
                            success: function(data) {
                                $('#div_fields_delivery').html(data);
                                init_material();
                                init_choices();
                            }
                        })
                    }
                }

                $("html").on("change", ".check_delivery_options", function() {
                    check_delivery_options();
                })

                $("html").on("change", ".select_delivery_options", function() {
                    select_delivery_options();
                })

                function select_delivery_category() {

                    if ($("#select_delivery_category").length) {
                        var value = $("#select_delivery_category").val();
                        var name = $("#select_delivery_category")[0].name;

                        // console.log("select_delivery_category");
                        // console.log("value: " + value);
                        // console.log("name: " + name);
                        // console.log("---------");

                        if (value) {

                            var parameters = {
                                f: "select_delivery_category",
                                val: value,
                                name: name,
                            };

                            $.ajax({
                                data: parameters,
                                type: "POST",
                                url: "https://camisetasargentinas.com/ajax.php",
                                success: function(data) {
                                    $('#div_select_delivery').html(data);
                                    init_choices();
                                    select_delivery_options();
                                    check_delivery_options();
                                }
                            })

                        } else {
                            $('#div_select_delivery').html('');
                        }
                    }
                }

                $("html").on("change", ".select_delivery_category", function() {
                    select_delivery_category();
                })

                function check_delivery_type() {

                    if ($("input[name='check_delivery_type']:checked").length) {
                        var value = $("input[name='check_delivery_type']:checked").val();

                        if (value) {
                            $('#div_select_delivery').html('');
                            $('#div_fields_delivery').html('');
                            $('#div_button_delivery').html('');

                            var parameters = {
                                f: "check_delivery_type",
                                val: value,
                            };

                            $.ajax({
                                data: parameters,
                                type: "POST",
                                url: "https://camisetasargentinas.com/ajax.php",
                                success: function(data) {
                                    $('#div_checkout_delivery').html(data);
                                    init_choices();
                                    select_delivery_category();
                                }
                            })
                        }
                    }
                }

                $("html").on("change", ".check_delivery_type", function() {
                    check_delivery_type();
                })

                $("html").on("click", ".order_online", function() {
                    var parameters = {
                        f: "form_delivery",
                        type_delivery: "online",
                    };

                    $.ajax({
                        data: parameters,
                        type: "POST",
                        url: "https://camisetasargentinas.com/ajax.php",
                        success: function(data) {
                            window.history.pushState("", "", "/finalizar-compra/datos/");
                            item_edit = 1;
                            reload_item();
                            reload_resume();
                            return false;
                        }
                    })
                })

                $("html").on("click", ".iframe_modal", function() {

                    item_edit = $(this).attr("html_item");
                    block_edit = $(this).attr("reload_block");

                    var type = $(this).attr("type");
                    var id_block = $(this).attr("id_block");
                    var id_html = $(this).attr("id_html");

                    if (type == "slider") {
                        slider_handler(id_block, false)
                    }

                    var url = $(this).attr("url");
                    var title = $(this).attr("title");

                    $('#iframe_modal').attr('type', type);
                    $('#iframe_modal').attr('id_block', id_block);
                    $('#iframe_modal').modal('toggle');
                    load_iframe("modal_iframe", url, title);
                    iframe_loaded();
                });

                $('#iframe_modal').on('hidden.bs.modal', function() {
                    var type = $(this).attr("type");
                    var id_block = $(this).attr("id_block");
                    if (type == "slider") {
                        slider_handler(id_block, true)
                    }
                    clearInterval(interval);
                    $('#title_modal_iframe').html('Cargando...');
                    $('#div_modal_iframe').html('<iframe scrolling="no" class="" frameBorder="0" style="overflow:hidden;margin-top:10px;width: 100%;" frameborder="0" width="100%" id="modal_iframe"></iframe>');
                    item_edit = null;
                    block_edit = null;
                })

                function reload_counters() {
                    $('.counter').each(function(d, obj) {
                        var countTo = $(this).attr("count_to");
                        $(this).html(countTo);
                    });
                }

                function reload_item() {
                    if (item_edit) {

                        var div = $("[html_item=" + item_edit + "]");

                        div.html($('#div_loading').html());

                        start_item(div)

                    } else if (block_edit) {
                        start_block($("[html_block=" + block_edit + "]"))
                    }
                }

                function slider_handler(id, start) {
                    if (start) {
                        $('#carousel-' + id).carousel('cycle');
                    } else {
                        $('#carousel-' + id).carousel('pause');
                    }
                }

                function load_iframe(name, url, title) {
                    window.edition = 0;
                    var iframe = $('#' + name);
                    if (iframe.length) {
                        $('#title_' + name).html(title);
                        iframe.attr('src', url);
                        return false;
                    }
                    return true;
                }

                function set_block() {
                    // console.log("set_block");
                }

                function each_iframe() {
                    $('.html_iframe iframe').each(function(d, obj) {
                        $(this).css('width', '100%');
                        $(this).css('height', "100%");
                        $(this).css('frameborder', "0");
                    });
                }

                function iframe_loaded() {
                    var iframe_id = document.getElementById('modal_iframe');
                    if (iframe_id) {
                        interval = setInterval(function() {
                            // console.log("iframe_loaded");
                            $('#modal_iframe').css('min-height', $('#modal_iframe').contents().height());
                        }, 500);
                    }
                }

                function drawer_left(init = 0) {
                    if (init) {
                        document.getElementById("drawer-left").style.right = "0";
                        document.getElementById("drawer-left-void").classList.add("d-block");
                        document.getElementById("drawer-left-void").classList.remove("d-none");
                        document.getElementsByTagName("body")[0].style.overflow = "hidden";
                    } else {
                        document.getElementById("drawer-left").style.right = "-500px";
                        document.getElementById("drawer-left-void").classList.add("d-none");
                        document.getElementById("drawer-left-void").classList.remove("d-block");
                        document.getElementsByTagName("body")[0].style.overflow = "auto";
                        $("#div_drawer_left").html("");
                    }
                }


                function cerrar(page) {

                    if (window.name == page) {
                        window.open('', '_parent', '');
                        window.close();
                    } else {
                        location.href = "https://camisetasargentinas.com/tienda/";
                    }

                }

                function get_menu() {
                    $("#div_drawer_left").html("<div class='mobile-menu'>" + $("#navigation").html()) + "</div>";
                }

                function get_cart() {
                    var parameters = {
                        f: "get_cart",
                    };

                    $.ajax({
                        data: parameters,
                        type: "POST",
                        url: "https://camisetasargentinas.com/ajax.php",
                        success: function(data) {
                            $("#div_drawer_left").html(data);
                        }
                    })
                }

                function reload_cart() {
                    var parametros = {
                        "f": 'reload_cart',
                    };

                    $.ajax({
                        data: parametros,
                        type: "POST",
                        url: "https://camisetasargentinas.com/ajax.php",
                        success: function(data) {
                            $('.num_cart').html(data);
                        }
                    })
                }

                $("html").on("click", ".back_order", function() {

                    var id_order = $(this).attr("id_order");

                    var parameters = {
                        f: "back_order",
                        id_order: id_order,
                    };

                    $.ajax({
                        data: parameters,
                        type: "POST",
                        url: "https://camisetasargentinas.com/ajax.php",
                        success: function(data) {
                            window.history.pushState("", "", "/finalizar-compra/pagar/");
                            item_edit = 1;
                            reload_item();
                            reload_resume()
                            return false;
                            // alert(data);
                        }
                    })
                    return false;
                });


                $("html").on("click", ".simulate_payment", function() {

                    swal({
                            title: "¿Estás seguro?",
                            text: "Esta acción es permanentemente.",
                            icon: "warning",
                            buttons: true,
                            dangerMode: true,
                        })
                        .then((action) => {
                            if (action) {
                                var id_order = $(this).attr("id_order");

                                $('#modal_loading').modal('toggle');

                                var parameters = {
                                    f: "simulate_payment",
                                    id_order: id_order,
                                };

                                $.ajax({
                                    data: parameters,
                                    type: "POST",
                                    url: "https://camisetasargentinas.com/ajax.php",
                                    success: function(data) {
                                        $('#modal_loading').modal('toggle');
                                        window.history.pushState("", "", "/finalizar-compra/pedido/" + id_order + "/");
                                        item_edit = 1;
                                        reload_item();
                                        reload_resume(id_order)
                                        $("html,body").animate({
                                                scrollTop: $($(".card-body")).offset().top,
                                            },
                                            0
                                        );
                                    }
                                })
                                return false;

                            }
                        });
                    return false;
                });

                $("html").on("click", ".finalize_order", function() {

                    var payment_method = $(this).attr("payment_method");
                    var id_order = $(this).attr("id_order");

                    $('#modal_loading').modal('toggle');

                    // $('#modal_loading').on('hide.bs.modal', function() {
                    //     return false;
                    // });

                    var parameters = {
                        f: "finalize_order",
                        payment_method: payment_method,
                    };

                    $.ajax({
                        data: parameters,
                        type: "POST",
                        url: "https://camisetasargentinas.com/ajax.php",
                        success: function(data) {
                            // alert(data);
                            $('#modal_loading').modal('toggle');
                            window.history.pushState("", "", "/finalizar-compra/pedido/" + id_order + "/");
                            item_edit = 1;
                            reload_item();
                            reload_resume(id_order)
                            $("html,body").animate({
                                    scrollTop: $($(".card-body")).offset().top,
                                },
                                0
                            );
                            return false;
                            // alert(data);
                        }
                    })
                    return false;
                })



                $("html").on("click", ".cart_remove", function() {

                    var id_cart = $(this).attr("id_cart");
                    var environment = $(this).attr("environment");
                    var reload_form = 0;

                    if ($(this).attr('reload_form') !== undefined) {
                        reload_form = 1;
                    }

                    var parameters = {
                        f: "cart_remove",
                        id_cart: id_cart,
                    };

                    $.ajax({
                        data: parameters,
                        type: "POST",
                        url: "https://camisetasargentinas.com/ajax.php",
                        success: function(data) {
                            if (environment == "checkout") {
                                reload_resume();
                                if (reload_form == 1) {
                                    item_edit = 1;
                                    reload_item();
                                }
                            } else if (environment == "cart") {
                                get_cart();
                                reload_cart();
                            }
                        }
                    })
                    return false;

                });

                $("html").on("click", ".qty_change", function() {

                    var qty_product = $("#qty_product").val();
                    var type = $(this).attr("type");

                    if (type == "up") {
                        qty_product++;
                        $(".qty-val").html(qty_product);
                        $("#qty_product").val(qty_product);
                    } else if (type == "down") {
                        if (qty_product != 1) {
                            qty_product--;
                            $(".qty-val").html(qty_product);
                            $("#qty_product").val(qty_product);
                        }
                    }
                })

                $("html").on("click", ".click_qty", function() {

                    var qty = $(this).attr("qty");
                    var id_cart = $(this).attr("id_cart");
                    var environment = $(this).attr("environment");
                    var reload_form = 0;

                    if ($(this).attr('reload_form') !== undefined) {
                        reload_form = 1;
                    }

                    var parameters = {
                        f: "change_qty",
                        qty: qty,
                        id_cart: id_cart,
                    };

                    $.ajax({
                        data: parameters,
                        type: "POST",
                        url: "https://camisetasargentinas.com/ajax.php",
                        success: function(data) {
                            if (environment == "checkout") {
                                reload_resume();
                                if (reload_form == 1) {
                                    item_edit = 1;
                                    reload_item();
                                }
                            } else if (environment == "cart") {
                                get_cart();
                            }
                        }
                    })
                    return false;

                });

                function reload_resume(id_order = null) {

                    var parameters = {
                        f: "checkout_resume",
                        id_order: id_order,
                    };

                    $.ajax({
                        data: parameters,
                        type: "POST",
                        url: "https://camisetasargentinas.com/ajax.php",
                        success: function(data) {
                            $('#div_resume').html(data);
                        }
                    })
                    return false;
                }

                $("html").on("click", ".navbar-click", function() {
                    drawer_left(1);
                    get_menu();
                    return false;
                });

                $("html").on("click", ".link_cart", function() {
                    drawer_left(1);
                    get_cart();
                    return false;
                });

                function close_window() {
                    if (confirm("¿Volver atras?")) {
                        close();
                    }
                }

                // $("html").on("click", ".navbar-click", function() {
                //     drawer_left(1)
                // });

                function init_inner(id_app) {

                    $('.ajax-inner').each(function(d, obj) {

                        var div = $(this);
                        var show = 1;

                        if (id_app) {
                            show = 0;
                            if (div.attr("id_app") == id_app) {
                                show = 1;
                            }
                        }
                        if (show == 1) {
                            // console.log(div);

                            // div.html($('#div_loading').html());

                            var explode = div.attr("attributes").split(",");

                            var parameters = {};

                            for (let i = 0; i < explode.length; i++) {
                                parameters[explode[i]] = div.attr(explode[i]);
                            }


                            $.ajax({
                                data: parameters,
                                type: "POST",
                                url: "https://camisetasargentinas.com/ajax.php",
                                success: function(data) {
                                    div.html(data);
                                    init_fields();
                                }
                            })
                        }
                    });
                }

                $("html").on("click", ".ag_dropdown", function(e) {
                    var op = $(this).attr("op");
                    var sw = $(this).attr("sw");
                    if (op == 1) {
                        $(this).find('.icon').addClass("fa-chevron-down").removeClass("fa-chevron-up");
                        $(this).attr("op", 0);
                        $("#" + sw).css("opacity", 0);
                    } else {
                        $(this).find('.icon').addClass("fa-chevron-up").removeClass("fa-chevron-down");
                        $(this).attr("op", 1);
                        $("#" + sw).css("opacity", 1);
                    }

                    return false;
                });

                $("html").on("click", ".products_categories", function(e) {

                    $('.loading-products').show();
                    $("#div_loading_products").css("opacity", "0.7");

                    var category = $(this).attr("category");
                    var page = $(this).attr("page");

                    if (page && category) {
                        if (page != 1) {
                            window.history.pushState("", "", "/tienda/filtrar/?categoria=" + category + "&page=" + page);
                        } else {
                            window.history.pushState("", "", "/tienda/filtrar/?categoria=" + category);
                        }
                    } else if (category) {
                        window.history.pushState("", "", "/tienda/filtrar/?categoria=" + category);
                    } else {
                        window.history.pushState("", "", "/tienda/");
                    }

                    var parameters = {
                        f: "handle_products_categories",
                        category: category,
                        page: page,
                    };

                    $.ajax({
                        data: parameters,
                        type: "POST",
                        url: "https://camisetasargentinas.com/ajax.php",
                        success: function(data) {
                            $('#div_load_products').html(data);
                            $('.loading-products').hide();
                            $("#div_loading_products").css("opacity", 1)
                        }
                    })
                    return false;
                })

                $("html").on("click", ".products_pagination", function(e) {

                    $('.loading-products').show();
                    $("#div_loading_products").css("opacity", "0.7");
                    $("html,body").animate({
                            scrollTop: $($("#scroll_products")).offset().top,
                        },
                        0
                    );

                    var category = $(this).attr("category");
                    var page = $(this).attr("page");

                    if (category) {
                        if (page != 1) {
                            window.history.pushState("", "", "/tienda/filtrar/?categoria=" + category + "&page=" + page);
                        } else {
                            window.history.pushState("", "", "/tienda/filtrar/?categoria=" + category);
                        }
                    } else {
                        if (page != 1) {
                            window.history.pushState("", "", "?page=" + page);
                        } else {
                            window.history.pushState("", "", "/tienda/");
                        }
                    }

                    var parameters = {
                        f: "handle_products_pagination",
                        category: category,
                        page: page,
                    };

                    $.ajax({
                        data: parameters,
                        type: "POST",
                        url: "https://camisetasargentinas.com/ajax.php",
                        success: function(data) {
                            $('#div_load_products').html(data);
                            $('.loading-products').hide();
                            $("#div_loading_products").css("opacity", 1)
                        }
                    })
                    return false;
                })

                function link_home() {
                    window.location.href = "/";
                    return false;
                }

                
                String.prototype.capitalize = function() {

                    return this.toLowerCase().replace(/(^|\s)[a-z\u00E0-\u00FC]/g, function(letter) {
                        return letter.toUpperCase();
                    });
                };

                String.prototype.firstUp = function() {

                    return this.substr(0, 1).toUpperCase() + this.substr(1);
                };

                function process_domain(input) {
                    let value = input.value;
                    let letters = value.replace(/[^a-zA-Z0-9-]/g, "");
                    input.value = letters;
                }

                function validateEmail($email) {
                    var emailReg = /^([\w-\.]+@([\w-]+\.)+[\w-]{2,4})?$/;
                    return ($email.length > 0 && emailReg.test($email));
                }

                function process_camel(input) {
                    input.value = input.value.capitalize();
                }

                function process_first(input) {
                    input.value = input.value.firstUp();
                }

                function process_int(input) {
                    let value = input.value;
                    let numbers = value.replace(/[^0-9]/g, "");
                    input.value = numbers;
                }

                function process_float(input) {
                    let value = input.value;
                    let numbers = value.replace(/[^0-9.]/g, "");
                    input.value = numbers;
                }

                function process_minus(input) {
                    input.value = input.value.toLowerCase();
                }

                function process_uper(input) {
                    input.value = input.value.toUpperCase();
                }

                jQuery.event.special.touchstart = {
                    setup: function(_, ns, handle) {
                        this.addEventListener("touchstart", handle, {
                            passive: !ns.includes("noPreventDefault")
                        });
                    }
                };
                jQuery.event.special.touchmove = {
                    setup: function(_, ns, handle) {
                        this.addEventListener("touchmove", handle, {
                            passive: !ns.includes("noPreventDefault")
                        });
                    }
                };
                jQuery.event.special.wheel = {
                    setup: function(_, ns, handle) {
                        this.addEventListener("wheel", handle, {
                            passive: true
                        });
                    }
                };
                jQuery.event.special.mousewheel = {
                    setup: function(_, ns, handle) {
                        this.addEventListener("mousewheel", handle, {
                            passive: true
                        });
                    }
                };
            </script>
        </body>

        </html>
